{
    "manifestVersion": 1,
    "id": "maven-build-validator-task",
    "publisher": "dbejarano120",
    "version": "1.0.0",
    "name": "Maven Speedup",
    "description": "Provide a command with the modules you want Maven to build.",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "maven-build-validator-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
              "ms.vss-distributed-task.tasks"
            ],
            "properties": {
              "name": "MavenBuildValidatorTask"
            }
          }
    ],
    "files": [
        {
            "path": "MavenBuildValidatorTask/node_modules/azure-devops-extension-sdk",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "MavenBuildValidatorTask",
            "addressable": true,
            "contentType": "application/zip"
        }
    ]
}