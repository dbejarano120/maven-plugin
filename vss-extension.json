{
    "manifestVersion": 1,
    "id": "custom-maven-build-task",
    "name": "Maven Custom Build",
    "version": "1.0.15",
    "publisher": "dbejarano120",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Provide a command with the modules you want Maven to build.",
    "categories": [
        "Azure Pipelines"
    ],
    "files": [
        {
            "path": "customavenbuildtask"
        }
    ],
    "contributions": [
        {
            "id": "custom-maven-build-validator-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "customavenbuildtask"
            }
        }
    ]
}